<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.restingrobot.tweetscannerpro">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/Theme.TweetScannerPro"
		android:name="com.restingrobot.tweetscannerpro.TweetScannerPro">
		<activity
			android:name=".ui.base.BaseActivity"
			android:label="@string/app_name"
			android:theme="@style/Theme.TweetScannerPro.NoActionBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>
